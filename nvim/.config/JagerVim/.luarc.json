{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "diagnostics.globals": [
    "vim"
  ],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME"
  ],
  "diagnostics.disable": [
    "trailing-space",
    "no-unknown",
    "undefined-field"
  ]
}
